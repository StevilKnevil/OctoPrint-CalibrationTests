<form class="form-horizontal">
    <div class="control-group">
        <label class="checkbox">
            <input type="checkbox" data-bind="checked: settings.plugins.calibrationtests.confirmAllGcode">Confirm all GCODE
        </label>
    </div>
    <div class="control-group" title="The temprature to heat the hot-end">
        <label class="control-label">Hot-End Temperature</label>
        <div class="controls">
            <input type="number" step=1 class="input-mini text-right" data-bind="value: settings.plugins.calibrationtests.hotEndTemp">
            <span class="add-on">degrees</span>
        </div>
    </div>
</form>